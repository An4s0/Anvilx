{
  "name": "{{name}}",
  "private": true,
  "scripts": {
    "dev": "pnpm run -r --parallel --aggregate-output dev",
    "build": "pnpm run -r build",
    "start": "pnpm run -r --parallel start",
    "lint": "pnpm run -r --parallel lint",
    "format": "pnpm prettier . --write"
  },
  "packageManager": "pnpm@{{pnpmVersion}}"
}
